{
  "expo": {
    "name": "Casio Calculator",
    "slug": "casio-calculator",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/casio-white-64.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.fissy.casio-calculator",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.fissy.casio-calculator",
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "fonts": [
      {
        "name": "Bungee",
        "family": "Bungee-Regular",
        "src": "./assets/fonts/Bungee-Regular.ttf"
      }
    ]
  }
}